<odoo>

<record id="generate_work_log_expense_view_form" model="ir.ui.view">
    <field name="name">Generate Work Log Expense</field>
    <field name="model">generate_work_log_expense</field>
    <field name="arch" type="xml">
        <form>
            <group name="group_1" colspan="4" col="2">
                <field name="type_id" invisible="1" />
                <field name="analytic_account_id" />
                <field name="date_start" />
                <field name="date_end" />
                <field name="date" />
            </group>
            <footer>
                <button
                        string="Confirm"
                        name="action_confirm"
                        type="object"
                        class="oe_highlight"
                        confirm="Are you sure?"
                    />
                or
                <button string="Cancel" class="oe_link" special="cancel" />
            </footer>
        </form>
    </field>
</record>

<record id="generate_work_log_expense_action" model="ir.actions.act_window">
    <field name="name">Generate Work Log Expense</field>
    <field name="res_model">generate_work_log_expense</field>
    <field name="view_mode">form</field>
    <field name="view_id" ref="generate_work_log_expense_view_form" />
    <field name="target">new</field>
</record>

</odoo>
