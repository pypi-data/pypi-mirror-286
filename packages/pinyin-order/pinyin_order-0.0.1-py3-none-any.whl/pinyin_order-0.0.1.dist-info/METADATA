```python
from pinyin_order import PinyinOrderStr, pinyin_sorted


result1 = PinyinOrderStr('æ±‰å­—å’ŒEnglish') == PinyinOrderStr('æ±‰å­å’Œenglish')  # True
result2 = PinyinOrderStr('æ±‰å­—') > PinyinOrderStr('hanzi')  # True
result3 = PinyinOrderStr('abcå•Š') <= PinyinOrderStr('ABCå“¦')  # True

strings = ['aå“¦', 'aå•Š', 'æ±‰å­—', 'è¿™æ˜¯abc', 'è€…æ—¶ABC', '123', 'abc', 'æ¼¢å­—', 'ğŸ€ğŸ”ğŸ€', ' ']
