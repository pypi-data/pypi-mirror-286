<?xml version="1.0" ?>
<odoo noupdate="1">

        <!--Default Notification Email template for new Customers -->
        <record id="mail_template_autobind_new_customers" model="mail.template">
            <field name="name">New Customers in Shopinvader</field>
            <field name="subject">${object.subject}</field>
            <field name="auto_delete" eval="True" />
            <field
            name="body_html"
        ><![CDATA[ Your shop account has been created!]]></field>
            <field name="model_id" ref="base.model_res_partner" />
        </record>

</odoo>
